{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*",
    "identity"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "oauth2": {
    "client_id": "imf0pehsqjibmn2",
    "scopes":["https://www.dropbox.com/oauth2/authorize?client_id=imf0pehsqjibmn2&response_type=token&redirect_uri=https://cbebjppepimnnpeelbdbnhddoneggjlb.chromiumapp.org/access_token';"]
  },
  "key": "cbebjppepimnnpeelbdbnhddoneggjlb"
}
